{
  "name": "@flyo/nitro-astro",
  "version": "1.0.0",
  "description": "Astro Framework",
  "main": "./dist/nitro-astro.js",
  "module": "./dist/nitro-astro.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/nitro-astro.mjs",
      "require": "./dist/nitro-astro.js"
    },
    "./FlyoNitroBlock.astro": {
      "types": "./src/components/FlyoNitroBlock.ts",
      "import": "./src/components/FlyoNitroBlock.ts",
      "require": "./src/components/FlyoNitroBlock.ts"
    },
    "./FlyoNitroPage.astro": {
      "types": "./src/components/FlyoNitroPage.ts",
      "import": "./src/components/FlyoNitroPage.ts",
      "require": "./src/components/FlyoNitroPage.ts"
    },
    "./FallbackComponent.astro": {
      "types": "./src/components/FallbackComponent.ts",
      "import": "./src/components/FallbackComponent.ts",
      "require": "./src/components/FallbackComponent.ts"
    }
  },
  "author": "Basil Suter <git@nadar.io>",
  "license": "MIT",
  "scripts": {
    "release": "npm publish --access=public",
    "build": "vite build"
  },
  "devDependencies": {
    "astro": "^3.4.3",
    "typescript": "5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flyocloud/nitro-astro"
  },
  "dependencies": {
    "@flyo/nitro-js": "^1.0.5"
  }
}
